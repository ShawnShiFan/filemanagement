<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pax.filemanagement.mapper.UserMapper">

    <insert id="insert" parameterType="com.pax.filemanagement.dao.UserInfo">
     INSERT INTO `user_info` VALUES (#{user_id}, #{user_name},#{user_sex},#{user_birth},#{user_phone},#{user_birth_address}, #{user_photo},#{create_time}, #{update_time})
    </insert>

</mapper>